<%= cell(PageHeadline::Cell::Item, nil, text: page_headline_text) %>
<p>
  <%= link_to_create_new_footer_section %>
</p>
<% if model %>
<table>
  <tr>
    <th>Id</th><th>Position</th><th>Contains</th><th></th><th></th>
  </tr>
  <% model.each do |footer_section| %>
  <tr>
    <td><%= footer_section.id %></td>
    <td><%= footer_section.position %></td>
    <td><%= footer_section.content.truncate(60) %></td>
    <td><%= link_to 'Edit', edit_admin_footer_section_path(footer_section) if can_update? %></td>
    <td><%= link_to 'Delete', delete_admin_footer_section_path(footer_section) if can_destroy? %></td>
  </tr>
  <% end %>
</table>
<% else %>
<p>No footer sections</p>
<% end %>
